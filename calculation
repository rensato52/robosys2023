import sys

ans1 = 0
ans2 = 0
ans3 = 0
for line in sys.stdin:
    try:
        ans1 += int(line)
        ans2 -= int(line)
        ans3 *= int(line)
    except:
        ans1 += float(line)
        ans2 -= float(line)
        ans3 *= float(line)

print(ans1,ans2,ans3)
