import sys
def tonum(s):
    try:
        return int(s)
    except:
        return float(s)

ans1 = 0
ans2 = 0

for line in sys.stdin:
    ans1 += tonum(line)
    ans2 -= tonum(line)

print(和:ans1 差:ans2)
